<svg width="1200" height="630" viewBox="0 0 1200 630" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="1200" height="630" fill="#0D1117"/>
  <rect x="100" y="100" width="1000" height="430" rx="20" fill="#171E2B"/>
  
  <!-- Logo -->
  <g transform="translate(150, 150) scale(1.5)">
    <rect width="200" height="200" rx="40" fill="#0D1117"/>
    <rect x="25" y="25" width="150" height="150" rx="30" fill="#171E2B"/>
    
    <!-- Parte da TV -->
    <rect x="35" y="35" width="130" height="100" rx="10" fill="#0D1117" stroke="#232F3B" stroke-width="5"/>
    
    <!-- Antena -->
    <path d="M70 35 L100 10 L130 35" stroke="#232F3B" stroke-width="6" stroke-linecap="round"/>
    
    <!-- Base da TV -->
    <rect x="75" y="135" width="50" height="20" rx="5" fill="#232F3B"/>
    
    <!-- Gráfico de tendência -->
    <path d="M50 100 L70 85 L90 90 L110 65 L130 50 L150 35" stroke="#E85D04" stroke-width="8" stroke-linecap="round" stroke-linejoin="round"/>
    
    <!-- Pontos no gráfico -->
    <circle cx="70" cy="85" r="4" fill="#FF8C32"/>
    <circle cx="90" cy="90" r="4" fill="#FF8C32"/>
    <circle cx="110" cy="65" r="4" fill="#FF8C32"/>
    <circle cx="130" cy="50" r="4" fill="#FF8C32"/>
  </g>
  
  <!-- Text -->
  <text x="600" y="200" font-family="Arial" font-size="60" fill="white" text-anchor="middle" font-weight="bold">
    Series<tspan fill="#E85D04">Trend</tspan>
  </text>
  
  <text x="600" y="280" font-family="Arial" font-size="30" fill="#E85D04" text-anchor="middle">
    Acompanhe as tendências das suas séries favoritas
  </text>
  
  <text x="600" y="340" font-family="Arial" font-size="24" fill="#AAAAAA" text-anchor="middle">
    Estatísticas em tempo real · Notificações · Lançamentos
  </text>
  
  <!-- Devices Mockup -->
  <g transform="translate(750, 220)">
    <rect width="300" height="600" rx="30" transform="scale(0.4) translate(0, 0)" fill="#0D1117"/>
    <rect x="15" y="15" width="270" height="570" rx="20" transform="scale(0.4) translate(0, 0)" fill="#171E2B"/>
    <rect x="30" y="50" width="240" height="60" rx="10" transform="scale(0.4) translate(0, 0)" fill="#232F3B"/>
    <text x="50" y="85" font-family="Arial" font-size="18" fill="white" transform="scale(0.4) translate(0, 0)">Séries em Tendência</text>
  </g>
  
  <!-- Download buttons -->
  <rect x="400" y="440" width="180" height="60" rx="10" fill="#E85D04"/>
  <text x="490" y="480" font-family="Arial" font-size="24" fill="white" text-anchor="middle">
    Google Play
  </text>
  
  <rect x="620" y="440" width="180" height="60" rx="10" fill="#E85D04"/>
  <text x="710" y="480" font-family="Arial" font-size="24" fill="white" text-anchor="middle">
    App Store
  </text>
</svg> 